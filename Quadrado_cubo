
import java.util.*;

public class Main{
    public static void main(String[] args){
        
        double n = 0.0;
        double quad = 0.0;
        double  cubo = 0.0;
        
        Scanner leitura = new Scanner(System.in);
        
        System.out.print("Digite o um numero: ");
        n = leitura.nextDouble();
        quad = Math.pow(n,2);
        cubo = Math.pow(n,3);
        
        System.out.printf("O quadrado de %f é : %f \n", n, quad );
        System.out.printf("O cubo de %f é : %f \n", n, cubo);
        
    }
}

        
            
        
        
            
        
