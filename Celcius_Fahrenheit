import java.util.*;
public class Main{
    public static void main(String[] args){
        double c; //celcius
        double f; //fareheith
        Scanner s = new Scanner(System.in);
        
        System.out.print("Digite a temperatura em Celcius ");
        c = s.nextDouble();
        
        f= (c * 9/5) + 32;
        
        System.out.printf("A temperatura em Farenheit ê de %.2f F°", f);
    }
}